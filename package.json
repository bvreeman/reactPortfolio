{
  "name": "reactportfolio",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "server.js",
  "scripts": {
    "dev": "node server.js",
    "client": "node scripts/start-client.js",
    "start": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
